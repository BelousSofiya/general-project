http://www.mulesoft.org/schema/mule/ee/core http://www.mulesoft.org/schema/mule/ee/core/current/mule-ee.xsd">
	<error-handler name="ua-1185-spacex-main-Error_Handler" doc:id="f89e211a-c986-46b9-ba3d-7384086b77fe" >
		<on-error-continue enableNotifications="true" logException="true" doc:name="On Error Continue" doc:id="424364d5-1bc3-40dd-8ce0-1400179f41ac" type="HTTP:NOT_FOUND">
			<set-variable value='#[%dw 2.0&#10;output application/json&#10;---&#10;{&#10;	"logType":"Error: ",&#10;	"code": 404,&#10;	"message": "Page Not Found",&#10;	"errorType": "HTTP:NOT_FOUND",&#10;	"dateTime": now(),&#10;	"description": "Error common project",&#10;	"source": ""&#10;}]' doc:name="ERROR_MESSAGE Error common project" doc:id="9c92b575-6629-4fc7-ab09-70299add6f8c" variableName="ERROR_MESSAGE" />
			<flow-ref doc:name="Flow Reference Common Project Impl" doc:id="8b1c937f-2cbe-4cac-a2d7-31e11841e196" name="ua-1185-spacex-common-project-impl-Sub_Flow"/>
		</on-error-continue>
	</error-handler>
	<error-handler name="ua-1185-spacex-main-mail-Error_Handler" doc:id="6406c846-5f0e-4993-830f-34594ad6cead" >
		<on-error-continue enableNotifications="true" logException="true" doc:name="On Error Continue" doc:id="ec399972-36e2-4bf0-a3ac-6abc7984e063" type="ANY">
			<!-- [STUDIO:"Variable Error mail connector"]<set-variable value='#[%dw 2.0&#10;output application/json&#10;&#45;&#45;-&#10;{&#10;	"logType":"Error: ",&#10;	"code": 400,&#10;	"message": "Bad Request",&#10;	"errorType": "BAD_REQUEST",&#10;	"dateTime": now(),&#10;	"description": "Error log-mail-connector ",&#10;	"source": "Common project"&#10;}&#93;' doc:name="Variable Error mail connector" doc:id="dae94e14-d6b7-4cbf-b007-5ec6ab7e3465" variableName="ERROR_MESSAGE" /> [STUDIO] -->
			<set-variable value='#[%dw 2.0&#10;output application/dw&#10;---&#10;(&#10;    (vars.ERROR_MESSAGE."logType" default "ERROR: ") ++ &#10;    (vars.ERROR_MESSAGE."description" default " "  ) ++&#10;    "." ++&#10;    (vars.ERROR_MESSAGE."source" default " " ) ++&#10;    ". Error details: {" ++ &#10;    (vars.ERROR_MESSAGE."message" default " ") ++ &#10;    "," ++ &#10;    (vars.ERROR_MESSAGE."code" default 500) ++ &#10;    "}"&#10;)]' doc:name="Set Variable mailErrorLog" doc:id="9a0c9b8f-d3b6-4d10-ad94-a19b96a02dee" variableName="mailErrorLog" />
			<logger level="ERROR" doc:name="mail connector errorLog" doc:id="cc0ffb69-4eb2-4629-bc9c-738700c56a79" message="#[%dw 2.0&#10;output application/java&#10;---&#10;vars.mailErrorLog]" />
		</on-error-continue>
	</error-handler>
	<error-handler name="ua-1185-spacex-main-salesforce-connector-Error_Handler" doc:id="5c6a4234-a771-4766-b4d4-55547ac53cd2" >
		<on-error-continue enableNotifications="true" logException="true" doc:name="On Error Continue" doc:id="9f6992fd-3850-4f76-824d-825e47dab1c7" type="ANY">
			<!-- [STUDIO:"Variable Error salesforce connector"]<set-variable value='#[%dw 2.0&#10;output application/json&#10;&#45;&#45;-&#10;{&#10;	"logType":"Error: ",&#10;	"code": 400,&#10;	"message": "Bad Request",&#10;	"errorType": "BAD_REQUEST",&#10;	"dateTime": now(),&#10;	"description": "Error log salesforce connector ",&#10;	"source": "Common project"&#10;}&#93;' doc:name="Variable Error salesforce connector" doc:id="f561514b-c1d3-4d64-addb-fac57698921e" variableName="ERROR_MESSAGE" /> [STUDIO] -->
			<set-variable value='#[%dw 2.0&#10;output application/dw&#10;---&#10;(&#10;    (vars.ERROR_MESSAGE."logType" default "ERROR: ") ++ &#10;    (vars.ERROR_MESSAGE."description" default " "  ) ++&#10;    "." ++&#10;    (vars.ERROR_MESSAGE."source" default " " ) ++&#10;    ". Error details: {" ++ &#10;    (vars.ERROR_MESSAGE."message" default " ") ++ &#10;    "," ++ &#10;    (vars.ERROR_MESSAGE."code" default 500) ++ &#10;    "}"&#10;)]' doc:name="Set Variable salesforceErrorLog" doc:id="8720f227-b4a0-4d85-a533-59b1ed6db6c1" variableName="salesforceErrorLog" />
			<logger level="ERROR" doc:name="mail connector errorLog" doc:id="bfdd407f-3e75-40f5-95f8-7d843dcb0d08" message="#[%dw 2.0&#10;output application/java&#10;---&#10;vars.salesforceErrorLog]" />
		</on-error-continue>
